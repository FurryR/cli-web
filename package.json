{
  "name": "cli-web",
  "version": "1.0.0",
  "description": "Frontend Terminal based on DOM",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint -c ./eslint.json ./src",
    "fix": "eslint -c ./eslint.json ./src --fix",
    "build": "rimraf dist && tsc -p ./tsconfig.json"
  },
  "jest": {
    "rootDir": "test",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurryR/cli-web.git"
  },
  "author": "å‡Œ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FurryR/cli-web/issues"
  },
  "homepage": "https://github.com/FurryR/cli-web#readme",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
